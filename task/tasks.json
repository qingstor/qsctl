      "Zone"
    ],
    "runtime_value": [
      "BucketList"
    ]
  },
  "Copy": {
    "type": "dependent",
    "path": "copy",
    "description": "the root copy task",
    "runtime_value": [
      "DestinationPath",
      "DestinationStorage",
      "DestinationType",
      "Pool",
      "SourcePath",
      "SourceStorage",
      "SourceType"
    ]
  },
  "CopyFile": {
    "type": "dependent",
    "path": "copy_file",
    "depend": "Copy",
    "description": "copy a file to DestinationStorage",
    "inherited_value": [
      "DestinationPath",
      "DestinationStorage",
      "SourcePath",
      "SourceStorage"
    ],
    "runtime_value": [
      "TotalSize"
    ]
  },
  "CopyLargeFile": {
    "type": "dependent",
    "path": "copy_file",
